      <Query>
        <DataSourceName>AW14</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        Production.ProductCategory.Name, Production.ProductSubcategory.Name AS Expr1, Production.Product.Name AS Expr2
FROM            Production.ProductCategory INNER JOIN
                         Production.ProductSubcategory ON Production.ProductCategory.ProductCategoryID = Production.ProductSubcategory.ProductCategoryID INNER JOIN
                         Production.Product ON Production.ProductSubcategory.ProductSubcategoryID = Production.Product.ProductSubcategoryID</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
